.modal#upload{tabindex: -1, role: 'dialog', aria: {hidden: true, labelledby: 'label_upload'}}
  .modal-dialog
    .modal-content
      .modal-header
        %button.close{data: {dismiss: 'modal'}}
          %span{aria: {hidden: true}}
            &times;
          %span.sr-only Close
        %h4.modal-title#label_upload Upload pictures

      = react_component :Upload, { slug: @gallery.slug }
